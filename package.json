{
  "name": "test-site",
  "version": "0.1.0",
  "private": true,
  "homepage": "./",
  "dependencies": {
    "@craco/craco": "^7.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "primereact": "^8.7.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.7.0",
    "react-router-dom": "^6.7.0",
    "react-scripts": "5.0.1",
